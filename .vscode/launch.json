    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Runtime",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                ".\\..\\preprocessor\\bytecode.bin"
            ]
        },
        {
            "name": "Launch Compiler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".\\cmd\\rexc\\main.go",
            "args": [
                "c:\\code\\json\\rules2.json",
                "-loglevel",
                "trace"
            ]
        }
    ]
}